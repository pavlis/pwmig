BIN=importpwmig
MAN1=importpwmig.1
ldlibs=-lseispp -lgclgrid $(DBLIBS) -lperf
SUBDIR=/contrib

include $(ANTELOPEMAKE)
include $(ANTELOPE)/contrib/include/antelopemake.local
CXXFLAGS+=-I$(BOOSTINCLUDE)
LDFLAGS+=-L$(BOOSTLIB)
OBJS=importpwmig.o
$(BIN) : $(OBJS)
	$(RM) $@
	$(CXX) $(CCFLAGS) -o $@ $(OBJS) $(LDFLAGS) $(LDLIBS)
